Task({ id: uuidv4(), text: task, date: selectedDate })\n    setTask('')\n  }\n\n  useEffect(() => {\n    const d = new Date()\n    d.setHours(23, 59, 0)\n    setSelectedDate(d)\n  }, [])\n\n  return (\n    <div className=\"task-wrapper\">\n      <TextField\n        id=\"task\"\n        label=\"Task\"\n        className=\"task-textfield\"\n        value={task}\n        onChange={e => setTask(e.target.value)}\n        margin=\"normal\"\n        variant=\"filled\"\n        inputProps={{\n          className: classes.input\n        }}\n      />\n      {selectedDate ? (\n        <DatePicker\n          className=\"task-date\"\n          selectedDate={selectedDate}\n          handleDateChange={handleDateChange}\n        />\n      ) : null}\n      <Button\n        color=\"primary\"\n        variant=\"contained\"\n        className=\"task-button\"\n        onClick={addTask}\n        disabled={!task.length}\n      >\n        Add Task\n      </Button>\n    </div>\n  )\n}\n\nTextFieldComponent.propTypes = {\n  addTask: PropTypes.func.isRequired\n}\n"},"lineCount":null}},"error":null,"hash":"9b1302f90cdbdea04362ac516235e3eb","cacheData":{"env":{}}}